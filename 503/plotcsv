#!/usr/local/bin/gnuplot

set output '503timeout.pdf'
set terminal pdf
set logscale x 2
set xlabel "Amount of Data [B]"
set ylabel "Time [s]"
set title 'send time'
#set key at 2048,1e-05
set style line 1 lc rgb '#0060ad' lt 1 lw 2 pt 5 ps 1.5   # --- blue
set style line 2 lc rgb '#dd181f' lt 1 lw 2 pt 5 ps 1.5   # --- red
plot '503timeout.csv' using 2:3 with linespoints ls 1 \
    title 'send time',\
    '503timeout.csv' using 2:5 with linespoints ls 2 \
    title 'recv time'
