#!/usr/local/bin/gnuplot

set output '502timeout_errorbars.pdf'
set terminal pdf
set logscale x 2
set xlabel "Amount of Data [kB]"
set ylabel "Time [s]"
set title 'send time'
set key at 2048,1e-05
set style line 1 lc rgb '#0060ad' lt 1 lw 2 pt 5 ps 1   # --- blue
set style line 2 lc rgb '#dd181f' lt 1 lw 2 pt 5 ps 1   # --- red
plot '502timeout.csv' using 1:2:3 w yerrorbars ls 1 title 'error', \
    '' using 1:2 w lines ls 1 title 'send time',\
    '502timeout.csv' using 1:4:5 w yerrorbars ls 2 title 'error',\
    '' using 1:4 w lines ls 2 title 'recv time'
